{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./client/*"
      ],
      "@/*": [
        ".client/*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "vue"
    ],
    "resolveJsonModule": true,
  },
  "exclude": [
    "node_modules"
  ]
}